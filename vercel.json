{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/clubs",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "dest": "server.js"
    },
    {
      "src": "/api/auth/user",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "dest": "server.js"
    }
  ]
}
